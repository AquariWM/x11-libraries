{
	"problemMatcher": [
		{
			"owner": "rustc",
			"severity": "warning",
			"pattern": [
				{
					"regexp": "^(warning|warn|error)(?:\\[(\\w+)\\])?: (.*)$",
					"severity": 1,
					"code": 2,
					"message": 3
				},
				{
					"regexp": "(?:^(?:\\w+:)?\\s+-->\\s+((?:[\\w-.]+\\/)+)?([\\w.]+):(\\d+):(\\d+)$)?",
					"fromPath": 1,
					"file": 2,
					"line": 3,
					"column": 4
				}
			]
		}
	]
}
