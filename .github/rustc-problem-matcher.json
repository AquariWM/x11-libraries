{
	"problemMatcher": [
		{
			"owner": "rustc",
			"severity": "warning",
			"pattern": [
				{
					"regexp": "^(warning|warn|error|note)(?:\\[(\\w+)\\])?:\\s+(.*)(?:\\n\\s+-->\\s+((?:[\\w-.]+\\/)*)([\\w-.]+):(\\d+):(\\d+)$)?",
					"severity": 1,
					"code": 2,
					"message": 3,
					"fromPath": 4,
					"file": 5,
					"line": 6,
					"column": 7
				}
			]
		}
	]
}
