{
	"problemMatcher": [
		{
			"owner": "rust-tests",
			"severity": "error",
			"pattern": [
				{
					"regexp": "^----[\\w\\s:]+----$"
				},
				{
					"regexp": "^(.*)$",
					"message": 1
				},
				{
					"regexp": "^.*$"
				},
				{
					"regexp": "^(?:.*,\\s+((?:[\\w\\-.]+\\/)*)([\\w\\-.]+):(\\d+):(\\d+)$)?",
					"fromPath": 1,
					"file": 2,
					"line": 3,
					"column": 4
				}
			]
		}
	]
}
