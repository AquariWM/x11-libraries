{
	"problemMatcher": [
		{
			"owner": "rust-tests",
			"severity": "error",
			"pattern": [
				{
					"regexp": "----[\\w\\s:]+----\\n([\\w\\s':(=)`,]+),\\s+((?:[\\w\\-.]+\\/)*)([\\w\\-.]+):(\\d+):(\\d+)",
					"message": 1,
					"fromPath": 2,
					"file": 3,
					"line": 4,
					"column": 5
				}
			]
		}
	]
}
